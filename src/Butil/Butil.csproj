<Project Sdk="Microsoft.NET.Sdk.Razor">

    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <Description>A set of JS utilities for Blazor developers</Description>
        <PackageLicenseExpression></PackageLicenseExpression>
        <PackageProjectUrl>https://github.com/msynk/Butil</PackageProjectUrl>
        <RepositoryUrl>https://github.com/msynk/Butil</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <Authors>msynk</Authors>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageLicenseFile>LICENSE</PackageLicenseFile>
        <Version>0.0.1</Version>
    </PropertyGroup>

    <ItemGroup>
        <None Include="..\..\LICENSE">
            <Pack>True</Pack>
            <PackagePath></PackagePath>
        </None>
        <None Include="..\..\README.md">
            <Pack>True</Pack>
            <PackagePath></PackagePath>
        </None>
    </ItemGroup>

    <ItemGroup>
        <PackageReference Include="Microsoft.JSInterop" Version="6.0.6" />
    </ItemGroup>

</Project>
